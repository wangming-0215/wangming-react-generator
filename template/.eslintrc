{
	"env": {
		"browser": true
	},
	"extends": ["airbnb"],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 8,
		"sourceType": "module"
	},
	"rules": {
    "linebreak-style": ["error", "windows"],
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "arrow-parens": "off",
    "global-require": "off",
    "import/no-extraneous-dependencies": "off" // 暂时
  }
}